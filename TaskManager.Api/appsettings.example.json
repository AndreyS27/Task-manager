{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=your_db_name;Username=your_user;Password=your_password"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Key": "your-secret-jwt-key-here-32-chars-min",
    "Issuer": "TaskManager",
    "Audience": "TaskManagerUsers",
    "ExpiresInMinutes": 60
  },
  "AllowedHosts": "*"
}
